{
  "name": "alkahest-ts-test-setup",
  "version": "0.0.2",
  "description": "A complete testing environment setup for Alkahest testing with Anvil, contract deployments, and test accounts.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "test": "bun test",
    "test:jest": "jest",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@viem/anvil": "^0.0.6",
    "bun-types": "^1.2.16",
    "viem": "^2.0.0",
    "zod": "^3.22.0"
  },
  "peerDependencies": {
    "arktype": "^2.0.0",
    "bun": "^1.0.0"
  },
  "peerDependenciesOptional": {
    "arktype": true
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/jquery": "^3.5.32",
    "@types/node": "^20.0.0",
    "jest": "^29.0.0",
    "typescript": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/VAR-META-Tech/alkahest-ts-testing-setup.git"
  },
  "keywords": [
    "alkahest",
    "testing",
    "blockchain",
    "anvil",
    "ethereum",
    "viem",
    "test-environment",
    "smart-contracts"
  ],
  "author": "VarMeta Tech",
  "license": "MIT"
}
